package day01;

import java.util.Random;

public class Test01 {
	public static void main(String[] args) {
		for (int i = 1; i < 6; i++) {
			for (int j = i; j < 6; j++) {
				for (int k = i; k < 6; k++) {
					if (((i * j * k) % 3) == 0) {
						System.out.println(i + " * " + j + " * " + k + " = " + i * j * k);
					}
				}
			}
		}
		
//		Random random1 = new Random();
